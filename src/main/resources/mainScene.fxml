<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="900.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.alivecrim.MainSceneController">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <children>
      <FlowPane hgap="10.0" style="-fx-background-color: yellow;" vgap="5.0">
         <children>
            <Button fx:id="button" mnemonicParsing="false" onAction="#bclick1" text="add device">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="button1" mnemonicParsing="false" onAction="#addLine" text="add line">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="printRegistry" mnemonicParsing="false" onAction="#print" text="print registry" />
         </children>
      </FlowPane>
      <ScrollPane>
         <content>
            <Pane fx:id="canvasPane" onMouseClicked="#canvasClick" prefHeight="9000.0" prefWidth="9000.0" style="-fx-background-color: grey;" />
         </content>
      </ScrollPane>
   </children>
</VBox>
