<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="900.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.alivecrim.MainSceneController">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <children>
      <FlowPane columnHalignment="CENTER" hgap="10.0" style="-fx-background-color: A8A7A8;" vgap="5.0">
         <children>
            <Button fx:id="button" contentDisplay="CENTER" mnemonicParsing="false" onAction="#bclick1" style="-fx-background-color: #363636; -fx-border-color: #cc527a;" text="add device" textFill="#ee048d">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Ubuntu Mono" size="14.0" />
               </font>
               <opaqueInsets>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </opaqueInsets>
            </Button>
            <Button fx:id="button1" mnemonicParsing="false" onAction="#addLine" style="-fx-background-color: #363636; -fx-border-color: #cc527a;" text="add line">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="printRegistry" mnemonicParsing="false" onAction="#print" style="-fx-background-color: #363636; -fx-border-color: #cc527a;" text="print registry" />
         </children>
         <VBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
      <ScrollPane>
         <content>
            <Pane fx:id="canvasPane" onMouseClicked="#canvasClick" prefHeight="9000.0" prefWidth="9000.0" style="-fx-background-color: white;" />
         </content>
      </ScrollPane>
   </children>
</VBox>
